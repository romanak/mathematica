Cell[TextData[{
 "Here is one way to do this. Essentially, the digit list must be reversed \
(read from right to left). The command ",
 StyleBox["Last", "MR"],
 " tears off the last member of the digit list (equivalently, the first \
member of the reversed digit list), and the command ",
 StyleBox["Rest", "MR"],
 " produces the rest of that list. In a live session, you can enter the \
outputs shown below and evaluate them."
}], "ExercisePart",ExpressionUUID->"a7c538dc-d930-4a80-bdb8-f2c741d80d3f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"displayCF", "[", "digitList_", "]"}], ":=", 
  RowBox[{"Fold", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Defer", "[", 
      RowBox[{"#2", "+", 
       FractionBox["1", "#1"]}], "]"}], "&"}], ",", 
    RowBox[{"Last", "[", "digitList", "]"}], ",", 
    RowBox[{"Rest", "[", 
     RowBox[{"Reverse", "[", "digitList", "]"}], "]"}]}], "]"}]}]], "Input",Ex\
pressionUUID->"704b7cca-72e3-4796-b899-93d9e7390d10"],

