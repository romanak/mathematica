Cell["Section 8.8", "Subsection",ExpressionUUID->"29fdda99-ce8f-4fd8-a6d2-80096a5e761d"],

Cell[TextData[{
 "Recall that the predicate command ",
 StyleBox["EvenQ", "MR"],
 " will return ",
 StyleBox["True", "MR"],
 " only if its argument is an even integer."
}], "Exercise",ExpressionUUID->"d5cbc706-4277-4652-8116-c52009416e0e"],

Cell[TextData[{
 "The command ",
 StyleBox["And", "MR"],
 " (",
 StyleBox["&&", "MR"],
 ") can be used to construct a pure predicate function."
}], "ExercisePart",ExpressionUUID->"8601e44a-f4db-469f-b01c-386ea0216258"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "f", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"f", "[", 
   RowBox[{"x_", "?", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"EvenQ", "[", "#", "]"}], "&&", 
       RowBox[{"#", ">", "10"}]}], "&"}], ")"}]}], "]"}], ":=", 
  "\"\<success\>\""}]}], "Input",ExpressionUUID->"eda973ba-a125-4545-87f8-\
18ecb97e3a38"],

Cell[TextData[{
 "The command ",
 StyleBox["Or", "MR"],
