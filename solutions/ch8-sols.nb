Cell[BoxData[{
 RowBox[{
  RowBox[{"bigNumber", "=", 
   RowBox[{"NestWhile", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"#", "+", 
       RowBox[{"FromDigits", "[", 
        RowBox[{"Reverse", "[", 
         RowBox[{"IntegerDigits", "[", "#", "]"}], "]"}], "]"}]}], "&"}], ",",
      " ", "196", ",", " ", 
     RowBox[{
      RowBox[{
